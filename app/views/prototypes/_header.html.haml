.jumbotron
  .container.text-center
    = image_tag("header_logo.png", alt: "PROTO TYPE - SHARE THE PROTOTYPE", height: "73px")

.filter-nav
  .container
    .row
      %ul.nav.nav-pills.col-lg-11
        %li{role: "presentation", class: "#{'active' if @active == 'popular'}"}
          = link_to 'Popular PROTO', root_path
        %li{role: "presentation", class: "#{'active' if @active == 'newest'}"}
          = link_to 'Newest PROTO', newest_index_path
      %button{type: "button", class: "btn btn-default col-lg-1"} View Tags
