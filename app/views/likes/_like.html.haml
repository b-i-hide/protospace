- if user_signed_in?
  - if @prototype.like_user(current_user)
    = button_to prototype_like_path(@prototype, @like), method: :delete, id: "like-button",class: "btn btn-default decrement-button", data: { disable_with: "sending..." }, remote: true do
      = image_tag("icon_heart_red.ico")
      %span.num#like-sum
      = "LIKE  #{@prototype.likes_count}"
  - else
    = button_to prototype_likes_path(@prototype), id: "like-button",class: "btn btn-default decrement-button", data: { disable_with: "sending..." }, remote: true do
      = image_tag("icon_heart.svg")
      %span.num#like-sum
      = "LIKE  #{@prototype.likes_count}"
- else
  = button_tag disabled: 'disabled', id: "like-button", class: "btn btn-default" do
    = image_tag("icon_heart_red.ico")
    %span.num#like-sum
      = "LIKE  #{@prototype.likes_count}"
